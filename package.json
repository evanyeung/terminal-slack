{
  "name": "terminal-slack",
  "version": "0.0.1",
  "description": "A terminal client for slack",
  "main": "main.js",
  "dependencies": {
    "blessed": "^0.1.56",
    "node-notifier": "^5.0.2",
    "request": "^2.55.0",
    "ws": "^3.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "pkg": "^4.3.0-beta.1",
    "shebang-loader": "0.0.1",
    "webpack": "^3.9.1",
    "webpack-node-externals": "^1.6.0"
  },
  "scripts": {
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "webpack && pkg dist/main.js -d -c package.json -o packed/terminal-slack --options no-warnings"
  },
  "pkg": {
    "scripts": [
      "dist/main.js",
      "node_modules/blessed/lib/widgets/*.js"
    ],
    "targets": [
      "node8-alpine-x64",
      "node8-linux-x64",
      "node8-macos-x64",
      "node8-win-x64"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evanyeung/terminal-slack.git"
  },
  "keywords": [
    "slack",
    "terminal",
    "client"
  ],
